{
  "name": "BJUT-Network-Autologin",
  "version": "0.6.1",
  "description": "BJUT 校园网自动认证 (Electron)",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.quitsense.bjutautologin",
    "productName": "BJUT Network Autologin",
    "files": [
      "main.js",
      "index.html",
      "utils/**/*"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "1.0.0-rc.10",
    "electron-store": "8.0.0",
    "keytar": "7.9.0",
    "node-wifi": "2.0.16",
    "qs": "6.11.0",
    "systeminformation": "^5.27.10"
  },
  "devDependencies": {
    "electron": "^38.2.0",
    "electron-builder": "^24.9.1"
  }
}
